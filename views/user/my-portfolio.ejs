  <script src="/js/chart.js"></script>
  <script>
    (function() {
      function drawChart(ctx, timeframe) {
        if (!ctx) return;

        var canvas = ctx.canvas || ctx;
        var context = canvas.getContext ? canvas.getContext('2d') : null;
        if (!context) return;

        var width = canvas.width;
        var height = canvas.height;
        context.clearRect(0, 0, width, height);

        var dataPoints = [];
        var variance = 0;

        if (timeframe === '1h') {
          variance = 0.08;
          for (var i = 0; i < 60; i++) {
            dataPoints.push(0.5 + (Math.sin(i / 5) * 0.15) + (Math.random() - 0.5) * variance);
          }
        } else if (timeframe === '4h') {
          variance = 0.12;
          for (var i = 0; i < 48; i++) {
            dataPoints.push(0.5 + (Math.sin(i / 8) * 0.2) + (Math.random() - 0.5) * variance);
          }
        } else if (timeframe === '1d') {
          variance = 0.15;
          for (var i = 0; i < 24; i++) {
            dataPoints.push(0.5 + (Math.sin(i / 4) * 0.25) + (Math.random() - 0.5) * variance);
          }
        } else if (timeframe === '1w') {
          variance = 0.2;
          for (var i = 0; i < 7; i++) {
            dataPoints.push(0.5 + (Math.sin(i / 2) * 0.3) + (Math.random() - 0.5) * variance);
          }
        }

        var padding = 20;
        var chartWidth = width - 2 * padding;
        var chartHeight = height - 2 * padding;

        context.strokeStyle = '#3b82f6';
        context.lineWidth = 3;
        context.lineCap = 'round';
        context.lineJoin = 'round';
        context.beginPath();

        dataPoints.forEach(function(value, index) {
          var x = padding + (index / (dataPoints.length - 1)) * chartWidth;
          var y = height - padding - value * chartHeight;

          if (index === 0) {
            context.moveTo(x, y);
          } else {
            context.lineTo(x, y);
          }
        });

        context.stroke();

        context.fillStyle = 'rgba(59, 130, 246, 0.1)';
        context.lineTo(width - padding, height - padding);
        context.lineTo(padding, height - padding);
        context.closePath();
        context.fill();
      }

      var chartCanvas = document.getElementById('vx_portfolio_chart');
      if (chartCanvas) {
        drawChart(chartCanvas, '1d');

        document.querySelectorAll('.vx_timeframe_btn').forEach(function(btn) {
          btn.addEventListener('click', function() {
            document.querySelectorAll('.vx_timeframe_btn').forEach(function(b) {
              b.classList.remove('vx_active');
            });
            this.classList.add('vx_active');

            var timeframe = this.dataset.timeframe;
            drawChart(chartCanvas, timeframe);
          });
        });
      }

      var depositBtn = document.querySelector('.vx_btn_secondary');
      if (depositBtn) {
        depositBtn.addEventListener('click', function() {
          window.location.href = '/user/deposit';
        });
      }

      var withdrawBtn = document.querySelector('.vx_btn_primary');
      if (withdrawBtn) {
        withdrawBtn.addEventListener('click', function() {
          window.location.href = '/user/withdraw';
        });
      }

      var holdingItems = document.querySelectorAll('.vx_holding_item');
      holdingItems.forEach(function(item) {
        item.addEventListener('click', function() {
          item.style.background = 'rgba(59, 130, 246, 0.05)';
          setTimeout(function() {
            item.style.background = '';
          }, 200);
        });
      });
    })();
  </script>
</body>
</html>
