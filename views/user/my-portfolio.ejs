<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>My Portfolio</title>
  <link rel="stylesheet" href="/css/styles.css">

  <style>
    * {
      box-sizing: border-box;
    }

    .vx_portfolio_wrapper {
      background: linear-gradient(135deg, #0a0f1e 0%, #151b2e 100%);
      min-height: 100vh;
      padding: 0;
      margin: 0;
    }

    .vx_portfolio_container {
      max-width: 1400px;
      margin: 0 auto;
      padding: 40px 20px;
    }

    .vx_portfolio_header {
      text-align: center;
      margin-bottom: 48px;
      animation: vx_fadeInDown 0.6s ease-out;
    }

    .vx_portfolio_header h2 {
      font-size: 42px;
      font-weight: 700;
      color: #ffffff;
      margin: 0 0 12px 0;
      letter-spacing: -0.5px;
    }

    .vx_portfolio_header p {
      font-size: 16px;
      color: #8b92a7;
      margin: 0;
      line-height: 1.6;
    }

    .vx_portfolio_top_grid {
      display: grid;
      grid-template-columns: 1fr 1fr;
      gap: 28px;
      margin-bottom: 32px;
    }

    .vx_portfolio_value_card {
      background: linear-gradient(135deg, #1a2332 0%, #0f1621 100%);
      border: 1px solid rgba(99, 179, 237, 0.15);
      border-radius: 16px;
      padding: 32px;
      position: relative;
      overflow: hidden;
      animation: vx_fadeInUp 0.6s ease-out 0.1s backwards;
    }

    .vx_portfolio_value_card::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      height: 2px;
      background: linear-gradient(90deg, transparent, #3b82f6, transparent);
      opacity: 0;
      transition: opacity 0.3s;
    }

    .vx_portfolio_value_card:hover::before {
      opacity: 1;
    }

    .vx_portfolio_value_label {
      font-size: 14px;
      color: #6b7280;
      margin-bottom: 16px;
      text-transform: uppercase;
      letter-spacing: 0.5px;
      font-weight: 500;
    }

    .vx_portfolio_value_display {
      margin-bottom: 24px;
    }

    .vx_portfolio_main_value {
      font-size: 56px;
      font-weight: 700;
      color: #3b82f6;
      line-height: 1;
      margin-bottom: 12px;
    }

    .vx_portfolio_change {
      display: flex;
      align-items: center;
      gap: 8px;
      font-size: 16px;
      font-weight: 600;
    }

    .vx_change_positive {
      color: #10b981;
    }

    .vx_change_negative {
      color: #ef4444;
    }

    .vx_change_icon {
      width: 20px;
      height: 20px;
    }

    .vx_portfolio_stats {
      display: grid;
      grid-template-columns: 1fr 1fr;
      gap: 16px;
      padding-top: 24px;
      border-top: 1px solid rgba(255, 255, 255, 0.05);
    }

    .vx_stat_item {
      display: flex;
      flex-direction: column;
    }

    .vx_stat_label {
      font-size: 12px;
      color: #6b7280;
      margin-bottom: 6px;
      text-transform: uppercase;
      letter-spacing: 0.5px;
    }

    .vx_stat_value {
      font-size: 20px;
      font-weight: 700;
      color: #ffffff;
    }

    .vx_holdings_card {
      background: linear-gradient(135deg, #1a2332 0%, #0f1621 100%);
      border: 1px solid rgba(99, 179, 237, 0.15);
      border-radius: 16px;
      padding: 32px;
      position: relative;
      overflow: hidden;
      animation: vx_fadeInUp 0.6s ease-out 0.2s backwards;
    }

    .vx_holdings_header {
      display: flex;
      justify-content: space-between;
      align-items: center;
      margin-bottom: 24px;
    }

    .vx_holdings_title {
      font-size: 20px;
      font-weight: 700;
      color: #ffffff;
      margin: 0;
    }

    .vx_holdings_count {
      background: rgba(59, 130, 246, 0.15);
      color: #3b82f6;
      padding: 4px 12px;
      border-radius: 12px;
      font-size: 12px;
      font-weight: 600;
    }

    .vx_holdings_list {
      list-style: none;
      padding: 0;
      margin: 0;
    }

    .vx_holding_item {
      display: flex;
      align-items: center;
      justify-content: space-between;
      padding: 16px 0;
      border-bottom: 1px solid rgba(255, 255, 255, 0.05);
      transition: all 0.3s;
    }

    .vx_holding_item:last-child {
      border-bottom: none;
    }

    .vx_holding_item:hover {
      padding-left: 8px;
    }

    .vx_holding_left {
      display: flex;
      align-items: center;
      gap: 16px;
      flex: 1;
    }

    .vx_holding_icon {
      width: 48px;
      height: 48px;
      background: linear-gradient(135deg, rgba(59, 130, 246, 0.2) 0%, rgba(37, 99, 235, 0.2) 100%);
      border: 1px solid rgba(59, 130, 246, 0.3);
      border-radius: 12px;
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 20px;
      font-weight: 700;
      color: #3b82f6;
    }

    .vx_holding_info h4 {
      font-size: 15px;
      font-weight: 600;
      color: #ffffff;
      margin: 0 0 4px 0;
    }

    .vx_holding_info p {
      font-size: 13px;
      color: #6b7280;
      margin: 0;
    }

    .vx_holding_right {
      display: flex;
      align-items: center;
      gap: 24px;
      text-align: right;
    }

    .vx_holding_value {
      display: flex;
      flex-direction: column;
    }

    .vx_holding_amount {
      font-size: 15px;
      font-weight: 600;
      color: #ffffff;
      margin-bottom: 4px;
    }

    .vx_holding_percentage {
      font-size: 13px;
      color: #6b7280;
    }

    .vx_holding_change {
      display: flex;
      flex-direction: column;
      align-items: flex-end;
      min-width: 80px;
    }

    .vx_holding_change_pct {
      font-size: 15px;
      font-weight: 700;
      display: flex;
      align-items: center;
      gap: 4px;
      margin-bottom: 4px;
    }

    .vx_change_positive_text {
      color: #10b981;
    }

    .vx_change_negative_text {
      color: #ef4444;
    }

    .vx_holding_usd {
      font-size: 13px;
      color: #3b82f6;
      font-weight: 600;
    }

    .vx_chart_section {
      background: linear-gradient(135deg, #1a2332 0%, #0f1621 100%);
      border: 1px solid rgba(99, 179, 237, 0.15);
      border-radius: 16px;
      padding: 32px;
      margin-bottom: 32px;
      animation: vx_fadeInUp 0.6s ease-out 0.3s backwards;
    }

    .vx_chart_header {
      display: flex;
      justify-content: space-between;
      align-items: center;
      margin-bottom: 24px;
    }

    .vx_chart_title {
      font-size: 20px;
      font-weight: 700;
      color: #ffffff;
      margin: 0;
    }

    .vx_timeframe_selector {
      display: flex;
      gap: 8px;
    }

    .vx_timeframe_btn {
      background: transparent;
      border: 1px solid rgba(99, 179, 237, 0.3);
      color: #9ca3af;
      padding: 8px 16px;
      border-radius: 8px;
      font-size: 13px;
      font-weight: 600;
      cursor: pointer;
      transition: all 0.3s;
    }

    .vx_timeframe_btn:hover {
      border-color: #3b82f6;
      color: #3b82f6;
    }

    .vx_timeframe_btn.vx_active {
      background: linear-gradient(135deg, #3b82f6 0%, #2563eb 100%);
      border-color: #3b82f6;
      color: #ffffff;
    }

    .vx_chart_container {
      position: relative;
      height: 300px;
      background: rgba(15, 23, 42, 0.3);
      border-radius: 12px;
      padding: 16px;
      border: 1px solid rgba(99, 179, 237, 0.1);
    }

    .vx_chart_loading {
      display: flex;
      align-items: center;
      justify-content: center;
      height: 100%;
      color: #6b7280;
      font-size: 14px;
    }

    .vx_allocation_grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
      gap: 20px;
      margin-bottom: 32px;
    }

    .vx_allocation_card {
      background: linear-gradient(135deg, #1a2332 0%, #0f1621 100%);
      border: 1px solid rgba(99, 179, 237, 0.15);
      border-radius: 16px;
      padding: 24px;
      text-align: center;
      animation: vx_fadeInUp 0.6s ease-out backwards;
      transition: all 0.3s;
    }

    .vx_allocation_card:nth-child(1) { animation-delay: 0.4s; }
    .vx_allocation_card:nth-child(2) { animation-delay: 0.5s; }
    .vx_allocation_card:nth-child(3) { animation-delay: 0.6s; }

    .vx_allocation_card:hover {
      transform: translateY(-4px);
      border-color: rgba(99, 179, 237, 0.4);
      box-shadow: 0 12px 40px rgba(59, 130, 246, 0.15);
    }

    .vx_allocation_label {
      font-size: 13px;
      color: #6b7280;
      margin-bottom: 12px;
      text-transform: uppercase;
      letter-spacing: 0.5px;
    }

    .vx_allocation_percentage {
      font-size: 40px;
      font-weight: 700;
      color: #3b82f6;
      margin-bottom: 12px;
    }

    .vx_allocation_amount {
      font-size: 14px;
      color: #9ca3af;
      font-weight: 500;
    }

    .vx_actions_section {
      display: flex;
      gap: 16px;
      margin-top: 32px;
      justify-content: center;
    }

    .vx_btn_primary {
      background: linear-gradient(135deg, #3b82f6 0%, #2563eb 100%);
      border: none;
      border-radius: 10px;
      padding: 14px 32px;
      color: #ffffff;
      font-size: 15px;
      font-weight: 600;
      cursor: pointer;
      transition: all 0.3s;
      display: flex;
      align-items: center;
      gap: 8px;
      text-decoration: none;
    }

    .vx_btn_primary:hover {
      transform: translateY(-2px);
      box-shadow: 0 8px 24px rgba(59, 130, 246, 0.4);
    }

    .vx_btn_secondary {
      background: transparent;
      border: 1px solid rgba(99, 179, 237, 0.3);
      border-radius: 10px;
      padding: 14px 32px;
      color: #ffffff;
      font-size: 15px;
      font-weight: 600;
      cursor: pointer;
      transition: all 0.3s;
      display: flex;
      align-items: center;
      gap: 8px;
      text-decoration: none;
    }

    .vx_btn_secondary:hover {
      background: rgba(59, 130, 246, 0.1);
      border-color: #3b82f6;
      color: #3b82f6;
    }

    .vx_btn_icon {
      width: 18px;
      height: 18px;
    }

    @keyframes vx_fadeIn {
      from {
        opacity: 0;
      }
      to {
        opacity: 1;
      }
    }

    @keyframes vx_fadeInDown {
      from {
        opacity: 0;
        transform: translateY(-20px);
      }
      to {
        opacity: 1;
        transform: translateY(0);
      }
    }

    @keyframes vx_fadeInUp {
      from {
        opacity: 0;
        transform: translateY(20px);
      }
      to {
        opacity: 1;
        transform: translateY(0);
      }
    }

    @media (max-width: 1200px) {
      .vx_portfolio_top_grid {
        grid-template-columns: 1fr;
      }

      .vx_portfolio_stats {
        grid-template-columns: 1fr 1fr;
      }
    }

    @media (max-width: 768px) {
      .vx_portfolio_container {
        padding: 24px 16px;
      }

      .vx_portfolio_header h2 {
        font-size: 32px;
      }

      .vx_portfolio_main_value {
        font-size: 44px;
      }

      .vx_portfolio_stats {
        grid-template-columns: 1fr;
      }

      .vx_chart_container {
        height: 250px;
      }

      .vx_allocation_grid {
        grid-template-columns: 1fr;
      }

      .vx_actions_section {
        flex-direction: column;
      }

      .vx_btn_primary,
      .vx_btn_secondary {
        width: 100%;
        justify-content: center;
      }

      .vx_holding_right {
        flex-direction: column;
        gap: 8px;
      }
    }

    @media (max-width: 480px) {
      .vx_portfolio_header h2 {
        font-size: 28px;
      }

      .vx_portfolio_main_value {
        font-size: 36px;
      }

      .vx_holding_item {
        padding: 12px 0;
      }

      .vx_holdings_header {
        flex-direction: column;
        align-items: flex-start;
        gap: 12px;
      }

      .vx_portfolio_value_card,
      .vx_holdings_card,
      .vx_chart_section {
        padding: 20px;
      }
    }
  </style>
</head>
