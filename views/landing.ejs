			if(!pre) return;

			function hidePreloader(){
				if(!pre) return;
				try{
					pre.style.transition = 'opacity 380ms ease';
					pre.style.opacity = '0';
					setTimeout(function(){
						if(pre && pre.parentNode){ pre.parentNode.removeChild(pre); }
					}, 420);
				}catch(e){
					if(pre && pre.parentNode) pre.parentNode.removeChild(pre);
				}
			}

			window.addEventListener('load', function(){
				setTimeout(hidePreloader, 120);
			});

			document.addEventListener('DOMContentLoaded', function(){
				setTimeout(function(){
					var visible = window.getComputedStyle(pre).getPropertyValue('opacity');
					if(visible !== '0') hidePreloader();
				}, 2000);
			});

			setTimeout(function(){ if(pre && pre.parentNode) pre.parentNode.removeChild(pre); }, 7000);
		}catch(err){
			try{ var p = document.getElementById('preloder'); if(p && p.parentNode) p.parentNode.removeChild(p); }catch(e){}
		}
	})();
