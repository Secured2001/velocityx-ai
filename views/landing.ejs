			for (let i = 0; i < miniCanvas.width; i += 10) {
				const x = i;
				const y = miniCanvas.height / 2 + Math.sin(i / 30) * 50 + (Math.random() - 0.5) * 20;
				if (i === 0) {
					ctx.moveTo(x, y);
				} else {
					ctx.lineTo(x, y);
				}
			}
			ctx.stroke();
		}
	}
	</script>
</body>
