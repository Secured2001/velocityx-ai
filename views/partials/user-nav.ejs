<!-- Main Content Wrapper -->
<div class="vx_nav_main_content" id="vx_nav_main_content">

<script>
  document.addEventListener('DOMContentLoaded', function() {
    const mobileToggle = document.getElementById('vx_nav_mobile_toggle');
    const sidebar = document.getElementById('vx_nav_sidebar');
    const mainContent = document.getElementById('vx_nav_main_content');
    const navLinks = document.querySelectorAll('.vx_nav_item a');

    function toggleSidebar() {
      sidebar.classList.toggle('vx_nav_active');
    }

    function closeSidebar() {
      sidebar.classList.remove('vx_nav_active');
    }

    mobileToggle.addEventListener('click', toggleSidebar);

    navLinks.forEach(function(link) {
      link.addEventListener('click', closeSidebar);
    });

    if (window.innerWidth > 768) {
      sidebar.classList.remove('vx_nav_active');
    }

    window.addEventListener('resize', function() {
      if (window.innerWidth > 768) {
        closeSidebar();
      }
    });

    const userMenu = document.getElementById('vx_nav_user_menu');
    const quickTrade = document.getElementById('vx_nav_quick_trade');
    const notifications = document.getElementById('vx_nav_notifications');

    if (userMenu) {
      userMenu.addEventListener('click', function() {
        console.log('User menu clicked');
      });
    }

    if (quickTrade) {
      quickTrade.addEventListener('click', function() {
        console.log('Quick trade clicked');
      });
    }

    if (notifications) {
      notifications.addEventListener('click', function() {
        console.log('Notifications clicked');
      });
    }

    if (typeof lucide !== 'undefined') {
      lucide.createIcons();
    }
  });
</script>
